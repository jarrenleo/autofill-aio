{
  "manifest_version": 3,
  "name": "Autofill AIO",
  "version": "1.1.0",
  "description": "Chrome extension that autofills checkout forms",
  "permissions": ["storage", "scripting", "tabs", "activeTab", "downloads"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "type": "module"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  }
}
